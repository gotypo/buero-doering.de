/*PRESSINFORMATION*/
.element-pressinformation .row {justify-content: flex-start;}
.element-pressinformation .text-overlay { z-index: 1000;}
.element-pressinformation .divider-with-image h1,
.element-pressinformation .divider-with-image h2,
.element-pressinformation .divider-with-image h6 { transform: skew(-10deg, -10deg);}
.element-pressinformation .divider-with-image h1,
.element-pressinformation .divider-with-image h2 {color: #fff; font-size: 110px; text-transform: uppercase; text-align: right;}
.element-pressinformation .divider-with-image h6 {color: var(--primary-color); font-size: 40px; text-transform: uppercase; text-align: right;}

.element-pressinformation .pressinformation-right {max-width: 650px; text-align: right; margin-right: 0;}

/*TEXT-IMAGE*/
.element-text-image h2 {margin-top: 0;}

/*CONTACT*/
.element-contact .contact-right {display: flex; align-content: center; align-items: center;}
.element-contact .communication {font-weight: 700; color: var(--primary-color);}
.element-contact .table-responsive {background-color: #F0F2F5; padding: 25px 50px; margin-bottom: 50px;}
.element-contact .table-responsive table {margin-bottom: 0;}
.element-contact .table-responsive td { color: var(--primary-color); font-weight: 600; min-width: 150px; display: inline-block; padding: 10px;}
.element-contact .table-responsive table td::after { display: none;}

/*CONTACT SLIDER*/
.contact-slider {position: absolute; right: -400px; top: 40px; z-index: 10000;}
.contact-slider .contact-slider-button {background-color: #529727; padding: 5px 20px; color: #fff; font-weight: 800; display: inline-block; transform: rotate(270deg);position: absolute;left: -80px;top: 40px;width: 120px;height: 40px; text-align: center;}
.contact-slider .contact-slider-button:hover {cursor: pointer;}
.contact-slider .contact-slider-content {width: 400px; padding: 60px 40px; background-color: transparent; -webkit-box-shadow: none; -moz-box-shadow:none; box-shadow:none;}
.contact-slider .contact-slider-content > * {display: block;}
.contact-slider .contact-slider-content > * > em {margin-right: 20px; font-size: 24px; color: var(--primary-color); }
.contact-slider .contact-slider-content .name {}
.contact-slider .contact-slider-content .image {margin-bottom: 60px;}
.contact-slider .contact-slider-content .phone {margin-bottom: 10px; display: flex; align-content: center; align-items: center;}
.contact-slider .contact-slider-content .email {display: flex; align-content: center; align-items: center;}

/*COLLAPSIBLE*/
.accordion-item {margin-bottom: 20px; border: 0; background-color: transparent; }
.accordion-item .accordion-header {padding: 25px 0; background-color: transparent; border: 0;}
.accordion-item .accordion-header h3 {margin: 0;}
.accordion-item .accordion-header a {width: 100%; display: block; position: relative; color: #fff;}
.accordion-item .accordion-header a:not(.collapsed) {color: var(--primary-color); }
.accordion-item .accordion-header a i {margin-right: 10px; width: 40px; height: 40px;}
.accordion-item .accordion-header a[aria-expanded="true"] i::before {content: "\f103";}
.accordion-item .accordion-header:hover a {cursor: pointer; color: var(--primary-color);  }

.accordion-item .accordion-body {border-left: 2px solid var(--primary-color); margin-left: 50px;}

.accordion-style-joboffer { border-bottom: 2px solid var(--primary-color); padding-bottom: 100px;}
.accordion-style-joboffer .accordion-item .accordion-body {margin-left: 0; bordeR: 0;}
.accordion-style-joboffer h3 {font-weight: 700; font-size: 50px; color: var(--primary-color);}
.accordion-style-joboffer .collapsing-content {clear: both;}
.accordion-style-joboffer .btn {float: right; min-width: 260px;}
.accordion-style-joboffer .btn i {margin-right: 30px; color: var(--primary-color); position: absolute; left: 30px; top: 21px;}
.accordion-style-joboffer .btn.collapsed span:last-of-type,
.accordion-style-joboffer .btn.collapsed .fa-angles-up {display: none;}
.accordion-style-joboffer .btn:not(.collapsed) span:first-of-type,
.accordion-style-joboffer .btn:not(.collapsed) .fa-angles-down {display: none;}

/*IMAGEGALLERY*/
.imagegallery .col-md-4 {padding: 15px; }
.element-imagegallery .col-md-4 {padding: 0 60px 100px 60px;}
.element-imagegallery .col-md-4 img {margin-bottom: 30px;}

main ul.blocks-gallery-grid .blocks-gallery-item:before {display: none;}

/*TEAM*/
/*.page-id-19896 #keyvisual picture { position: fixed; right: 0; top: 0; min-width: 100%; min-height: 100%;}*/

.team {padding-top: 100px;}
.team .masked-image-team {
    mask-image: url('../images/mask_image_portrait.svg');
    -webkit-mask-image: url('../images/mask_image_portrait.svg');
    mask-position: 0px 16%;
    -webkit-mask-position: 0px 16%;
    mask-size: cover;
    -webkit-mask-size: cover;
    mask-repeat: no-repeat;
    -webkit-mask-repeat: no-repeat;
    width: 100%; height: auto; background: var(--secondary-color); background-size: 100% 80%;}
.team .name {text-transform: uppercase; font-weight: 700; font-size: 60px; line-height: 1; color: var(--primary-color); position: relative; bottom: 80px; text-align: left; left: 0; margin-left: 0px; transform: skew(0deg, -14deg);}
.team .position {font-weight: 700; font-size: 40px; line-height: 1; color: #fff; position: relative; bottom: 80px; text-align: left; left: 0; margin-left: 0px; transform: skew(0deg, -14deg);}
.team .team-right .fact {color: var(--primary-color); margin-bottom: 5px; }
.team .team-right .fact strong {color: #fff; margin-right: 10px; }

/*DOWNLOADS*/
.element-downloads {padding-top: 0;}
.element-downloads .el-download {font-size: 25px; display: flex; margin-bottom: 15px; max-width: calc(50% - 30px);}
.element-downloads a:nth-of-type(odd) {margin-right: 30px;}
.element-downloads a:nth-of-type(even) {margin-left: 30px;}
.element-downloads span {display: inline-block;}
.element-downloads em {display: inline-block; width: 55px; height: 47px; margin-right: 50px;}

/* APPOINTMENT */
#sln-salon.sln-salon--l {margin: 0;}
#sln-salon label.sln-service {margin-bottom: 0; padding-top: 15px;}
#sln-salon label.sln-service h3 {font-size: 18px;}
#sln-salon .sln-steps-description {margin-top: 0 !important; margin-bottom: 0 !important;}

/*CONTENTSWIPER*/
.element-contentswiper {position: relative; padding: 40px 150px; margin: 100px 0;}
.element-contentswiper .contentswiper .swiper-wrapper {padding: 100px 0;}
.element-contentswiper .contentswiper {position: relative; overflow: hidden;}
.element-contentswiper .contentswiper .swiper-slide img {
    mask-image: url('../images/mask_slider_element.svg');
    -webkit-mask-image: url('../images/mask_slider_element.svg');
    mask-repeat: no-repeat;
    -webkit-mask-repeat: no-repeat;
    mask-size: 100%;
    -webkit-mask-size: 100%;
    mask-position: top;
    -webkit-mask-position: top;
    width: 100%; height: auto; transform: scale(1); transition: all .5s ease-in-out;}
.element-contentswiper .contentswiper .swiper-slide.swiper-slide-next {position: relative; z-index: 100;}
