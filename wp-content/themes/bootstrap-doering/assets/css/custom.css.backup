    0% { opacity: 0; }
    100% { opacity: 1; }
}

.single-post .site-container {background-color: #222;}
.single-post .site-container > .site-inner {z-index: 199; background-color: transparent;}

.single-post  footer#footer {background-color: #222 !important; padding: 50px 0; z-index: 198; overflow:visible; position: relative;}


/*SCHEDULE*/
.schedule {position: relative; margin: 100px 0;}
.schedule .schedule-item {position: relative; display: flex; margin-bottom: 60px;}
.schedule .schedule-item .date-element-wrapper {position: relative; display: flex; flex-direction: column; justify-content: space-between; margin-right: 60px;}
.schedule .schedule-item .date-element {position: relative; min-width: 270px;}
.schedule .schedule-item .date-element-wrapper:after {content: ""; width: 2px; height: calc(100% - 100px); position: absolute; top: 150px; left: calc(50% - 1px); background-color: var(--primary-color); }
.schedule div.schedule-item:last-of-type .date-element-wrapper:after {display: none;}
.schedule .schedule-item .schedule-body {margin-bottom: 60px; margin-right: 0; max-width: 65%;}
.schedule .schedule-item .schedule-body h1,
.schedule .schedule-item .schedule-body h2,
.schedule .schedule-item .schedule-body h3,
.schedule .schedule-item .schedule-body h4,
.schedule .schedule-item .schedule-body h5 {font-size: 60px;}

.schedule.scheduletabs .schedule-item {height: 100%; opacity: .5; top: 0; transition: opacity 2s ease-in-out;}
.schedule.scheduletabs .schedule-item.active {left: 0; position: relative; max-width: 80%;}
.schedule.scheduletabs .schedule-item.active + .schedule-item:hover,
.schedule.scheduletabs .schedule-item.active {opacity: 1;}
.schedule.scheduletabs .schedule-item .schedule-body {display: none; opacity: 0;}
.schedule.scheduletabs .schedule-item.active .schedule-body {display: block; margin-bottom: 180px; opacity: 1; min-height: 40vh;}
.schedule.scheduletabs .schedule-item .date-element-wrapper:after {height: calc(100% - 300px);}

