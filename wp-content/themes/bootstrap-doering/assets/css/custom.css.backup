/*COLORS*/
:root {
    --primary-color: #FF1F79;
    --secondary-color: #72187F;

    --primary-font-color: #fff;
    --dark-font-color: #1C3035;
    --secondary-font-color: var(--primary-color);
    --swiper-theme-color: var(--primary-color);
}

/*COLORS*/
:root.colorscheme-1 {
    --primary-color: #FF1F79;
    --secondary-color: #72187F;

    --secondary-font-color: var(--primary-color);
    --swiper-theme-color: var(--primary-color);
}

:root.colorscheme-2 {
    --primary-color: #FF6B5D;
    --secondary-color: #002C4C;

    --secondary-font-color: var(--primary-color);
    --swiper-theme-color: var(--primary-color);

}
:root.colorscheme-3  {
    --primary-color: #AFCA0B;
    --secondary-color: #007C67;

    --secondary-font-color: var(--primary-color);
    --swiper-theme-color: var(--primary-color);
}

/*primary gradients*/
.colorscheme-1 .gradient-primary { background: linear-gradient(90deg, rgba(255,31,121,1) 0%, rgba(128,16,61,0) 100%);}
.colorscheme-2 .gradient-primary { background: linear-gradient(90deg, rgba(255,107,93,1) 0%, rgba(0,44,76,0) 100%);}
.colorscheme-3 .gradient-primary { background: linear-gradient(90deg, rgba(175,202,11,1) 0%, rgba(0,124,103,0) 100%);}

