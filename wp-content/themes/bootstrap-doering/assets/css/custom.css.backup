/*COOKIE BANNER*/
#klaro {position: relative; z-index: 10000;}

#klaro .cm-modal { transform: translateY(250%);}
#klaro.show .cm-modal {
    animation: slide-up 1s;
    animation-timing-function: linear;
    animation-delay: 0s;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
}

@keyframes slide-up {
    0% {
        transform: translateY(250%);
    }
    100% {
        transform: translateY(-50%);
    }
}

.klaro p {font-size: 14px;}
.klaro .cookie-notice {background: #fff !important;  border-radius: 0 !important; max-width: 450px !important;}
.klaro .cookie-modal .cm-btn,
.klaro .cookie-notice .cm-btn {border-radius: 0 !important;  margin-bottom: 10px;}

.klaro .cookie-modal .cm-modal {max-width: 760px !important; background: #333 !important; border-radius: 0 !important; box-shadow: none !important; padding: 8rem 2rem 6.5rem 2rem; mask-image: url('../images/mask_news_element_image.svg'); -webkit-mask-image: url('../images/mask_news_element_image.svg');mask-repeat: no-repeat;-webkit-mask-repeat: no-repeat; -webkit-mask-size: contain; mask-size: contain;}
.klaro .cookie-modal p,
.klaro .cookie-modal strong,
.klaro .cookie-modal h1,
.klaro .cookie-modal h2,
.klaro .cookie-modal ul,
.klaro .cookie-modal li,
.klaro .cookie-notice p,
.klaro .cookie-notice strong,
.klaro .cookie-notice h1,
.klaro .cookie-notice h2,
.klaro .cookie-notice ul,
.klaro .cookie-notice li {color: #fff !important;}
.klaro .cookie-modal .cm-modal .cm-header h1 {font-weight: 700;}
.klaro .cookie-modal .cm-modal .cm-body ul.cm-apps li.cm-app span.cm-required,
.klaro .cookie-modal .cm-modal .cm-body ul.cm-apps li.cm-app p.purposes {color: #fff !important;}
.klaro .cookie-modal .cm-modal .cm-header {border: 0 !important; padding: 2rem !important;}
.klaro .cookie-modal .cm-modal .cm-body {border: 0; padding: 1rem 2rem !important;}
.klaro .cookie-modal .cm-modal .cm-footer {border: 0 !important; padding: 2rem !important;}
.klaro .cookie-modal a {color: var(--primary-color) !important; text-decoration: underline !important;}
.klaro .cookie-notice .cm-btn,
.klaro .cookie-notice .cm-btn.cm-btn-info {background-color: transparent !important; border: 1px solid #fff !important; padding: 15px 30px !important;}

.klaro .cookie-modal .cm-modal .cm-footer-buttons {display: flex; flex-flow: column-reverse !important;}

.klaro .cookie-modal .cm-btn,
.klaro .cookie-notice .cm-btn,
.klaro .cookie-modal .cm-btn.cm-btn-success {position: relative; background-color: transparent !important; border: 3px solid var(--primary-color); font-size: 25px; padding: 15px 30px; font-weight: 700; border-radius: 6px !important; text-transform: uppercase; color: #fff; display: inline-block; }
.klaro .cookie-modal .cm-btn:hover {cursor: pointer; border-color: var(--secondary-color);}
.klaro .cookie-modal .cm-btn.cm-btn-accept {padding: 6px 10px;margin-right: .5em;border-style: none;padding: 0.4em;font-size: 1em;cursor: pointer;}

.klaro .cookie-modal .cm-app-input:checked + .cm-app-label .slider, .klaro .cookie-notice .cm-app-input:checked + .cm-app-label .slider {background-color: var(--primary-color) !important;}
.klaro .cookie-modal .cm-list-input:checked + .cm-list-label .slider, .klaro .cookie-notice .cm-list-input:checked + .cm-list-label .slider {background-color: var(--primary-color) !important;}
.klaro .cookie-modal .cm-list-label .slider::before {background-color: #fff !important;}
.klaro .cookie-modal .half-checked + .cm-list-label .slider::before {background-color: var(--primary-color) !important;}

.klaro .cookie-notice .cn-buttons {display: flex; flex-flow: column-reverse !important;}
.klaro .cookie-notice .cm-link {margin: 10px 0 30px 0; display: block; color: var(--primary-color) !important;}
.klaro .cookie-notice .cm-btn {display: block !important; width: 100% !important;}
.klaro .cookie-notice:not(.cookie-modal-notice) .cn-body .cn-ok {display: block !important;}
.klaro .cookie-notice .cm-btn.cm-btn-success {display: flex; flex-flow: column !important;  background: var(--primary-color) !important; color: #fff !important;}

.klaro .cm-powered-by a {color: #333 !important;}

.klaro .context-notice .cm-btn.cm-btn-success {background-color: var(--primary-color) !important;}
.klaro .context-notice .cm-btn.cm-btn-success-var {background-color: var(--secondary-color) !important;}

/*MAIN*/
html {scrollbar-color: var(--primary-color) transparent;}
html.colorscheme-1 {scrollbar-color: var(--primary-color) var(--secondary-color);}
html.colorscheme-2 {scrollbar-color: var(--primary-color) var(--secondary-color);}
html.colorscheme-3 {scrollbar-color: var(--primary-color) var(--secondary-color);}

html {overflow-x: hidden; text-rendering: optimizeLegibility; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; position: relative; height: 100%;}
body {overflow-x: hidden; font-family: proxima-nova, sans-serif; font-size: 22px; color: #fff; background-color: #333; }

h1, h2, h3, h4, h5 {color: #fff; font-weight: 400; position: relative; text-transform: uppercase; font-family: proxima-nova-extra-condensed, sans-serif;}
h1 {font-size: 110px; line-height: 1;}
h2 { font-size: 110px; font-weight: 900;}
h3 {font-size: 40px; color: var(--primary-color);}

img {max-width: 100%; height: auto;}

p {line-height: 1.4; max-width: 1000px;}
p.has-text-align-right {margin: 0 0 0 auto;}

img {max-width: 100%; height: auto;}

iframe {border: 0;}

p,
main ul,
figcaption,
table { color: #fff;}

a {color: var(--primary-color); text-decoration: none;}
a:hover {text-decoration: none; color: var(--secondary-color);}
p a {font-weight: 700;}

.text-primary {color: var(--primary-color); }

main ul {padding-left: 70px;}
main ul li {list-style-type: none; position: relative;}
main ul li:before {width: 20px; height: 3px; left: -40px; top: 14px; position: absolute; content: " "; background-color: var(--primary-color);}

blockquote {font-style: italic; padding: 4rem 4rem 4rem 9rem;position: relative; max-width: 1000px;margin: 0 auto;}
blockquote:before {position: absolute; content: "”"; left: 0; top: 0; color: var(--primary-color); font-size: 300px; line-height: 1;}


*:hover { -webkit-transition: all 0.3s ease-in-out; -moz-transition: all 0.3s ease-in-out; -ms-transition: all 0.3s ease-in-out; -o-transition: all 0.3s ease-in-out; transition: all 0.3s ease-in-out;}
*:after { -webkit-transition: all 0.3s ease-in-out; -moz-transition: all 0.3s ease-in-out; -ms-transition: all 0.3s ease-in-out; -o-transition: all 0.3s ease-in-out; transition: all 0.3s ease-in-out;}
*:focus { outline: none;}

::selection { background: #000000;  color: #fff;}
::-moz-selection { background: #000000;  color: #fff;}

/*MASKED IMAGE*/
.masked-image {position: relative;}
/*.masked-image:after {content: ""; height: calc(100% + 2px); width: calc(100% + 2px); position: absolute; top: -1px; left: 0px; z-index: 200;background-image: url("data:image/svg+xml,%3C%3Fxml version='1.0' encoding='utf-8'%3F%3E%3Csvg version='1.1' id='Ebene_1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' x='0px' y='0px' preserveAspectRatio='none' viewBox='0 0 426 568.2' style='enable-background:new 0 0 426 568.2;' xml:space='preserve'%3E%3Cstyle type='text/css'%3E .st0%7Bfill:%23FFFFFF;%7D%0A%3C/style%3E%3Cg%3E%3Cpath class='st0' d='M425.5,0.7H0.3v0.4l425.2,144.1V0.7z'/%3E%3Cpath class='st0' d='M425.5,423.9L1.5,567.7h424V423.9z'/%3E%3C/g%3E%3C/svg%3E%0A");}*/

/*BUTTONS*/
button {background-color: transparent; border: 0; color: var(--primary-color); }

.linkbuttons a,
.btn {position: relative; background-color: transparent; border: 3px solid var(--primary-color); font-size: 25px; padding: 15px 30px; font-weight: 700; border-radius: 6px; text-transform: uppercase; color: #fff; display: inline-block;}

.linkbuttons a:hover,
.btn:hover {color: #fff; border-color: var(--secondary-color);}

.btn-primary {position: relative; background-color: var(--primary-color); border-color: var(--primary-color);}
.btn-primary:hover {background-color: #fff; border-color: var(--primary-color); color: var(--primary-color);}

.btn-external {background-color: #fff; color: var(--primary-color); position: relative;}
.btn-external:after {content: ""; position: absolute; color: var(--primary-color); top: 20px; right: 35px;  width: 22px; height: 22px;  background-image: url("data:image/svg+xml,%3Csvg width='1em' height='1em' viewBox='0 0 16 16' class='bi bi-box-arrow-up-right' fill='%23232870' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' d='M8.636 3.5a.5.5 0 0 0-.5-.5H1.5A1.5 1.5 0 0 0 0 4.5v10A1.5 1.5 0 0 0 1.5 16h10a1.5 1.5 0 0 0 1.5-1.5V7.864a.5.5 0 0 0-1 0V14.5a.5.5 0 0 1-.5.5h-10a.5.5 0 0 1-.5-.5v-10a.5.5 0 0 1 .5-.5h6.636a.5.5 0 0 0 .5-.5z'/%3E%3Cpath fill-rule='evenodd' d='M16 .5a.5.5 0 0 0-.5-.5h-5a.5.5 0 0 0 0 1h3.793L6.146 9.146a.5.5 0 1 0 .708.708L15 1.707V5.5a.5.5 0 0 0 1 0v-5z'/%3E%3C/svg%3E"); background-size: contain; background-repeat: no-repeat;}
.btn-external:hover {opacity: 0.8;  border-color: var(--primary-color); color: var(--primary-color);}

.btn-send {padding: 24px 30px 24px 30px;}

.btn-close {background: transparent url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16' fill='%23fff'%3e%3cpath d='M.293.293a1 1 0 011.414 0L8 6.586 14.293.293a1 1 0 111.414 1.414L9.414 8l6.293 6.293a1 1 0 01-1.414 1.414L8 9.414l-6.293 6.293a1 1 0 01-1.414-1.414L6.586 8 .293 1.707a1 1 0 010-1.414z'/%3e%3c/svg%3e") center/1em auto no-repeat;}

/*ALIGNMENT*/
.align-left {text-align: left;}
.align-center{text-align: center;}
.align-right {text-align: right;}

