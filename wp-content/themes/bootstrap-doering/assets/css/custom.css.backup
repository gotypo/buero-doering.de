/*TABLE */
table th,
table td {padding: 20px; position: relative;}

table:not(.table-condensed) th:after {content: ""; position: absolute; bottom: 0; left: 0; height: 3px; width: calc(100% - 20px); background-color: var(--primary-color);}
table:not(.table-condensed) td:after {content: ""; position: absolute; bottom: 0; left: 0; height: 2px; width: calc(100% - 20px); background-color: var(--primary-color);}

/*SHORTCUTS*/
.shortcuts {margin-bottom: 100px;}
.shortcuts .buttons {display: flex; justify-content: space-between;}
.shortcuts .buttons .btn {min-width: 240px;}
.shortcuts .buttons .btn:nth-of-type(1) {animation-delay: .2s;}
.shortcuts .buttons .btn:nth-of-type(2) {animation-delay: .4s;}
.shortcuts .buttons .btn:nth-of-type(3) {animation-delay: .6s;}
.shortcuts .buttons .btn:nth-of-type(4) {animation-delay: .8s;}

/*NEWS*/
.element.el-news {padding-bottom: 30px;}
.element.el-news .el-news-inner {position: relative; display: block; background-color: #F0F2F5; height: 100%; padding: 40px;}
.element.el-news .el-news-inner .image {margin-bottom: 50px;}
.element.el-news .el-news-inner h3 {margin-bottom: 50px;}

/*BLOG SINGLE*/
.blog-single h1,
.blog-single h2,
.blog-single h3,
.blog-single h4,
.blog-single h5 {clear: both;}

/*DATE*/
.date-element {position: absolute; top: 0; left: 0; line-height: 1;}
.date-element .day {font-size: 140px; color: #fff;}
.date-element .rotate-90 {transform: rotate(270deg); position: relative;left: 50px; top: -20px; display: inline-block; text-align: right;}
.date-element .month {position: relative; left: 0px; bottom: 50px; font-size: 35px; font-weight: 700; color: var(--primary-color); display: block; margin-bottom: 10px;}
.date-element .year { position: relative; left: 0px; bottom: 60px;font-size: 35px; font-weight: 700; color: var(--primary-color); display: block;}

/*POSTS*/
.el-posts#news {margin-bottom: 100px;}
.el-posts .post-item {display: flex; position: relative; padding: 120px 0 0px 0; min-height: 560px; margin: 0 0 60px 0; --bs-gutter-x: 0;}
.el-posts .post-item .image {align-self: stretch;}
.el-posts .post-item .text { align-self: stretch; max-width: 700px; padding-left: 60px; display: flex; flex-direction: column;align-content: center;justify-content: space-around;}
.el-posts .post-item .text h3 {font-weight: 700; color: #fff;}
.el-posts .post-item .image img {visibility: hidden;}
.home .el-posts div.post-item:last-of-type {margin-bottom: 0;}

.el-posts .masked-overlay {
    display: flex;
    z-index: -1;
    position: absolute;
    width: 100%;
    height: 90%;
    left: 0;
    top: 10%;
    clip-path: polygon(0 30%, 100% 0, 100% 70%, 0% 100%);
    /*mask-image: url('../images/mask-news-element-complete.svg');*/
    /*mask-size: 100%;*/
}

.el-posts .masked-overlay .masked-image {
    align-self: stretch;
    overflow: hidden;
    display: flex;
    align-items: end;
}
.el-posts .masked-overlay .masked-image  img {height: 80%; width: auto; max-width: unset;}

/*SINGLE */
.single-post .site-container > .site-inner,
.single-post .site-container > .site-inner .content {padding-top: 0;}
.single-post .site-container > .site-inner .single-image {margin-bottom: 60px; position: relative;}
.single-post .site-container > .site-inner .single-image img {width: 100%; opacity: 0; animation: fadeImage 3s forwards; animation-delay: 1s;}

.single-post .single-image {position: sticky;}
